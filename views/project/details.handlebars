<h3>{{ project.title }}</h3>

<img src="{{project.imageUrl}}" width=250 alt="Image">
<div class="">
  {{#if userMatch}}
  <a class="nav-link" href="/project/{{project.id}}/edit">Edit</a>
  {{/if}}
</div>
<div class="">
  <a href="/profile/{{project.creator.slug}}">
    <h5>By: {{ project.creator.name }}</h5>
  </a>
  <h5>Date: {{ project.created }}</h5>
  <h5>Category: {{ project.category }}</h5>
</div>
<div class="">


</div>
<div class="">
  <p>{{ project.material }}</p>
  <p> {{ project.reflection }}</p>
</div>
