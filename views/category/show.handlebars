<h3>{{ patterns.category }}</h3>
<div class="">
  <h4>Patterns</h4>
  {{#each patterns as |pattern|}}
  <div class="">

    <a href="/pattern/{{ pattern.id }}" class="">
      <img src="{{pattern.imageUrl}}" width=300 alt="Image">
      <h5>{{ pattern.title }}</h5>
    </a>
  </div>
  {{/each}}

</div>

<div class="">
  <h4>Projects</h4>
  {{#each projects as |project|}}
  <div class="">
    <a href="/pattern/{{ pattern.id }}" class="">
      <img src="{{project.imageUrl}}" width=300 alt="Image">
      <h5>{{ project.title }}</h5>
    </a>
  </div>
  {{/each}}

</div>
